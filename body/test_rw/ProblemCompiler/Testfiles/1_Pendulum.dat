Interval X[4];
X[0] = [0.0,2*Pi];
X[1] = [-2.8,2.8];
X[2] = [-1.7,1.7];
X[3] = [-1.3,1.3];
Interval U;
U = [-2.,2.];
Real gamma;
gamma = 0.0125;
f: (x,u) in (X,U) to y[4]
{
	y[0] = x[1];
	y[1] = -sin(x[0])-cos(x[0])*u-2*gamma*x[1];
	y[2] = x[3];
	y[3] = u;
}

phi: (t,x0,u) in ([0,.3],X,U) to x[4]
{
	initialvalue(x,x0);
	diff(x,t) = f(x,u);
}

